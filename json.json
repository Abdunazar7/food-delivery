{
  "info": {
    "_postman_id": "YOUR_COLLECTION_ID_HERE",
    "name": "Food Delivery App - Authentication (NestJS/TypeORM)",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "1. User Auth",
      "item": [
        {
          "name": "User Sign Up",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"firstName\": \"Ali\",\n  \"lastName\": \"Valiyev\",\n  \"email\": \"user@example.com\",\n  \"password\": \"12345\"\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/auth/user/signup",
              "host": ["{{BASE_URL}}"],
              "path": ["auth", "user", "signup"]
            }
          }
        },
        {
          "name": "User Sign In",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"{{user_email}}\",\n  \"password\": \"{{user_password}}\"\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/auth/user/signin",
              "host": ["{{BASE_URL}}"],
              "path": ["auth", "user", "signin"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Sign In dan keyin Access Token va User ID ni saqlash uchun skript shu yerga qo'shiladi"
                ]
              }
            }
          ]
        },
        {
          "name": "User Refresh",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"userId\": {{user_id}}\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/auth/user/refresh",
              "host": ["{{BASE_URL}}"],
              "path": ["auth", "user", "refresh"]
            }
          }
        },
        {
          "name": "User Sign Out",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"userId\": {{user_id}}\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/auth/user/signout",
              "host": ["{{BASE_URL}}"],
              "path": ["auth", "user", "signout"]
            }
          }
        }
      ]
